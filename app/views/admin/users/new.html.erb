<h1>New user</h1>
<% form_for([:admin, @user]) do |f| %>
<%= f.error_messages %>
<p>
    <b>ID</b>
    <br/>
    <%= f.text_field :id %>
</p>
<p>
    <b>Name</b>
    <br/>
    <%= f.text_field :name %>
</p>
<p>
    <b>Email</b>
    <br/>
    <%= f.text_field :email %>
</p>
<p>
    <b>Created At</b>
    <br/>
    <%= f.text_field :created_at %>
</p>
<p>
    <b>Updated At</b>
    <br/>
    <%= f.text_field :updated_at %>
</p>
<p>
    <b>User Id</b>
    <br/>
    <%= f.text_field :user_id %>
</p>
<p>
    <b>Login</b>
    <br/>
    <%= f.text_field :login %>
</p>
<p>
    <b>Crypted Password</b>
    <br/>
    <%= f.text_field :crypted_password %>
</p>
<p>
    <b>Password Salt</b>
    <br/>
    <%= f.text_field :password_salt %>
</p>
<p>
    <b>Login Count</b>
    <br/>
    <%= f.text_field :login_count %>
</p>
<p>
    <b>Last Request At</b>
    <br/>
    <%= f.text_field :last_request_at %>
</p>
<p>
    <b>Last Login at</b>
    <br/>
    <%= f.text_field :last_login_at %>
</p>
<p>
    <b>Last Login IP</b>
    <br/>
    <%= f.text_field :login_count %>
</p>
<p>
    <b>Current Login IP</b>
    <br/>
    <%= f.text_field :current_login_ip %>
</p>
<p>
    <%= f.submit 'Create' %>
</p>
<% end %>
<%= link_to 'Back', admin_users_path %>